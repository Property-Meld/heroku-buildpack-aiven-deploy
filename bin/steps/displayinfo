#!/usr/bin/env bash

indent() {
  sed -u 's/^/       /'
}


puts_step "displayinfo ENV: "
for X in $(ls $env_dir);do
    echo $X | indent
done

echo "pwd: $(pwd)"
for X in $(ls $(pwd));do
    echo $X | indent
done

echo "build_dir: $build_dir"
for X in $(ls $build_dir);do
    echo $X | indent
done
echo "BIN_DIR: $BIN_DIR"
for X in $(ls $BIN_DIR);do
    echo $X | indent
done
